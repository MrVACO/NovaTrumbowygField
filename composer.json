{
    "name": "mr-vaco/nova-trumbowyg-field",
    "description": "Trumbowyg WYSIWYG editor",
    "keywords": [
        "laravel",
        "nova",
        "trumbowyg"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "MrVaco\\NovaTrumbowygField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrVaco\\NovaTrumbowygField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
